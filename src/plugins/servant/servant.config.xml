<ServantConfiguration name="servant" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../acquire/servant.config.xsd">
	<Module name="adbroker" type="corba::servant" filename="/lib/qtPlatz/plugins/ScienceLiaison/adbroker$(configuration).dll"/>
	<Module name="adcontroller" type="corba::servant" filename="/lib/qtPlatz/plugins/ScienceLiaison/adcontroller$(configuration).dll"/>
	<Module name="tofcontroller" type="corba::servant" filename="/lib/qtPlatz/tofcontroller$(configuration).dll"/>
	<Configuration>
		<Configuration name="adbroker" type="" component="">
			<Component name="adbroker" module="adbroker"/>
		</Configuration>
		<Configuration name="adcontroller" type="" component="">
			<Component name="adcontroller" module="adcontroller"/>
			<Configuration name="InstrumentConfiguration">
   			<Configuration name="tofcontroller.1" id="tof.1" type="object_ref" ns_name="tofcontroller.manager"/>
			</Configuration>
		</Configuration>
		<Configuration name="tofcontoller" type="orbLoader" ns_name="tofcontroller.manager" component="">
			<Component name="tofcontroller" module="tofcontroller"/>
		</Configuration>
	</Configuration>
</ServantConfiguration>
