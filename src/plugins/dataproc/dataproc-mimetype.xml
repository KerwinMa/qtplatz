<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
  <mime-type type="application/octet-stream">
        <comment>binary document</comment>
  </mime-type>
  <mime-type type="application/adfs">
    <alias type="text/xml"/>
      <sub-class-of type="application/octet-stream"/>
      <comment> qtPlatz data file</comment>
      <glob pattern="*.adfs"/>
      <alias type="text/xml"/>
  </mime-type>
  <mime-type type="application/qtms">
    <alias type="text/xml"/>
    <sub-class-of type="application/octet-stream"/>
    <comment>InfiTOF Spectrum</comment>
    <glob pattern="*.qtms"/>
