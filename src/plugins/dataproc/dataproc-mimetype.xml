  </mime-type>
  <mime-type type="application/data">
        <sub-class-of type="application/octet-stream"/>
        <comment>MC4 data file</comment>
        <glob pattern="*.data"/>
    </mime-type>
    <mime-type type="application/plt">
      <sub-class-of type="application/octet-stream"/>
      <comment>InfiTOF data file</comment>
      <glob pattern="*.plt"/>
    </mime-type>
  <mime-type type="application/txt">
       <sub-class-of type="application/octet-stream"/>
       <comment>Spectrum file</comment>
       <glob pattern="*.txt"/>
    </mime-type>
