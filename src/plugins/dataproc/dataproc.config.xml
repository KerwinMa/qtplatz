<!---<AcquireConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="acquire.config.xsd">
	-->
<DataprocConfiguration name="dataproc" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/config.xsd">
  <Module name="qtwidgets" type="sharedlib" filename="/ScienceLiaison/qtwidgets$(configuration).dll"/>
  <Module name="mc4" type="sharedlib" filename="/ScienceLiaison/mc4$(configuration).dll"/>
  <Module name="adtxtfactory" type="sharedlib" filename="/ScienceLiaison/adtxtfactory$(configuration).dll"/>
  <Module name="addatafile" type="sharedlib" filename="/ScienceLiaison/addatafile$(configuration).dll"/>
  <Module name="infitof" type="sharedlib" filename="/MSI/infitofspectrometer$(configuration).dll"/>
  <Configuration name="ProcessMethodEditors">
    <Configuration name="ProcessMethodView">
      <title>Process Method</title>
      <Component type="widget" interface="qtwidgets::ProcessMethodView" module="qtwidgets" loader="adplugin">
	<Component type="qml" QUrl="/qtwidgets/qml/ProcessMethodEditor.qml" />
      </Component>
    </Configuration>
    <Configuration name="ElementalCompUI">
      <title>Elemental Comp.</title>
      <Component type="widget" interface="qtwidgets::ElementalCompositionForm" module="qtwidgets" loader="adplugin"/>
    </Configuration>
    <Configuration name="MSCalibrationUI">
      <title>MS Calibration</title>
      <Component type="widget" interface="qtwidgets::MSCalibrationForm" module="qtwidgets" loader="adplugin"/>
    </Configuration>
  </Configuration>
  <Configuration name="dataproviders">
    <Configuration name="mc4provider">
      <pattern>*.data</pattern>
      <Component type="dataprovider" interface="mc4::dataprovider" module="mc4" loader="adplugin"/>
    </Configuration>
    <!---
    <Configuration name="infitofprovider">
      <pattern>*.plt</pattern>
      <Component type="dataprovider" interface="infitof::dataprovider" module="infitof" loader="adplugin"/>
    </Configuration>
    -->
    <Configuration name="txtprovider">
      <pattern>*.txt</pattern>
      <Component type="dataprovider" interface="n/a" module="adtxtfactory" loader="adplugin"/>
    </Configuration>
    <Configuration name="native_data_provider">
      <pattern>*</pattern>
      <Component type="dataprovider" interface="n/a" module="addatafile" loader="adplugin"/>
    </Configuration>
  </Configuration>
</DataprocConfiguration>
