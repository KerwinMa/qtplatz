<!---<AcquireConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="acquire.config.xsd">
-->
<DataprocConfiguration name="dataproc" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/config.xsd">
  <Module name="qtwidgets" type="sharedlib" filename="/MS-Cheminformatics/qtwidgets$(configuration).dll"/>
  <Module name="qtwidgets2" type="sharedlib" filename="/MS-Cheminformatics/qtwidgets2$(configuration).dll"/>
  <Module name="mc4" type="sharedlib" filename="/MS-Cheminformatics/mc4$(configuration).dll"/>
  <Module name="adtexfile" type="sharedlib" filename="/MS-Cheminformatics/adtxtfactory$(configuration).dll"/>
  <Module name="addatafile" type="sharedlib" filename="/MS-Cheminformatics/addatafile$(configuration).dll"/>
  <Module name="fticr_factory" type="sharedlib" filename="/MS-Cheminformatics/fticr$(configuration).dll"/>
  <Module name="mzxml_factory" type="sharedlib" filename="/MS-Cheminformatics/mzxml$(configuration).dll"/>
  <Module name="CompassXtract" type="sharedlib" filename="/MS-Cheminformatics/compassxtract$(configuration).dll"/>
  <Module name="infitof" type="sharedlib" filename="/MSI/infitofspectrometer$(configuration).dll"/>
  <Configuration name="ProcessMethodEditors">
    <Configuration name="CentroidUI">
      <title>Centroid</title>
      <Component type="widget" interface="qtwidgets::CentroidForm" module="qtwidgets" loader="adplugin"/>
    </Configuration>
    <Configuration name="TargetUI">
      <title>Targeting</title>
      <Component type="widget" interface="qtwidgets::TargetForm" module="qtwidgets" loader="adplugin"/>
    </Configuration>
    <Configuration name="IsotopeUI">
      <title>Isotope</title>
      <Component type="widget" interface="qtwidgets::IsotopeForm" module="qtwidgets" loader="adplugin"/>
    </Configuration>
    <Configuration name="ElementalCompUI">
      <title>Elemental Comp.</title>
      <Component type="widget" interface="qtwidgets::ElementalCompositionForm" module="qtwidgets" loader="adplugin"/>
    </Configuration>
    <Configuration name="MSCalibrationUI">
      <title>MS Calibration</title>
      <Component type="widget" interface="qtwidgets2::MSCalibrationForm" module="qtwidgets2" loader="adplugin"/>
    </Configuration>
    <Configuration name="PeakMethodUI">
      <title>Peak Find</title>
      <Component type="widget" interface="qtwidgets::PeakMethodForm" module="qtwidgets" loader="adplugin"/>
    </Configuration>
  </Configuration>
  <Configuration name="MSCalibSummaryWidget">
     <title>Calibration result summary</title>
     <Component type="widget" interface="qtwidgets::MSCalibSummaryWidget" module="qtwidgets" loader="adplugin"/>      
  </Configuration>
  <Configuration name="MSMCalibSummaryWidget">
     <title>Calibration result summary</title>
     <Component type="widget" interface="qtwidgets2::MSCalibSummaryWidget" module="qtwidgets2" loader="adplugin"/>      
  </Configuration>
  <Configuration name="dataproviders">
    <Configuration name="mc4provider">
      <Component type="dataprovider" interface="mc4::dataprovider" module="mc4" loader="adplugin"/>
    </Configuration>
    <Configuration name="txtprovider">
      <Component type="dataprovider" interface="n/a" module="adtxtfactory" loader="adplugin"/>
    </Configuration>
    <Configuration name="fticr_provider">
      <Component type="dataprovider" interface="n/a" module="fticr_factory" loader="adplugin"/>
    </Configuration>
    <Configuration name="mzxml_provider">
      <Component type="dataprovider" interface="n/a" module="mzxml_factory" loader="adplugin"/>
    </Configuration>
    <Configuration name="compassxtract_provider">
      <Component type="dataprovider" interface="n/a" module="CompassXtract" loader="adplugin"/>
    </Configuration>
    <Configuration name="native_data_provider">
      <Component type="dataprovider" interface="n/a" module="addatafile" loader="adplugin"/>
    </Configuration>
  </Configuration>
</DataprocConfiguration>
