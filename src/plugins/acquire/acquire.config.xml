<!---<AcquireConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="acquire.config.xsd">
	-->
<AcquireConfiguration name="acquire" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/config.xsd">
	<Module name="adtofms.dll" type="sharedlib" filename="/lib/qtPlatz/plugins/ScienceLiaison/adtofms$(configuration).dll"/>
	<Module name="qtwidgets.dll" type="sharedlib" filename="/lib/qtPlatz/plugins/ScienceLiaison/qtwidgets$(configuration).dll"/>
	<Module name="infitofui" type="sharedlib" filename="/lib/qtPlatz/plugins/MSI/infitoftune$(configuration).dll"/>
	<Configuration name="monitor_tab">
		<Configuration name="log_book" type="tab" readonly="true">
			<title lang="jp">Log</title>
			<title lang="en">Log</title>
			<Component name="qtwidgets::LogWidget" type="widget" loader="adplugin" interface="adplugin::ui::iLog" module="qtwidgets.dll"/>
			<datasource name="broker.logger" type="cosnaming::name" interface="Broker::Logger">adbroker.manager</datasource>
		</Configuration>
		<Configuration name="infitof_monitor" type="tab">
			<title lang="en">InfiTOF</title>
			<title lang="jp">InfiTOF</title>
			<Component name="infitofui::monitor_ui" type="widget" loader="adplugin" interface="adplugin::ui::iMonitor" module="infitofui"/>
		</Configuration>
		<Configuration name="sequence_monitor" type="tab" readonly="true">
			<title lang="jp">Sequence</title>
			<title lang="en">Sequence</title>
			<Component name="SequencesForm" type="widget" loader="adplugin" interface="adplugin::ui::iSequencesForm" module="qtwidgets.dll"/>
		</Configuration>
	</Configuration>
</AcquireConfiguration>
